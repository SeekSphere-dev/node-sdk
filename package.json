{
  "name": "seeksphere-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript Node.js SDK for SeekSphere API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "release": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "seeksphere",
    "search",
    "api",
    "sdk",
    "typescript",
    "nodejs",
    "database",
    "query"
  ],
  "author": "SeekSphere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seeksphere/seeksphere-node-sdk.git"
  },
  "homepage": "https://github.com/seeksphere/seeksphere-node-sdk#readme",
  "bugs": {
    "url": "https://github.com/seeksphere/seeksphere-node-sdk/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
